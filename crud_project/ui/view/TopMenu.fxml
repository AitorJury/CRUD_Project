<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="crud_project.ui.controller.MenuBarController"
      stylesheets="@../res/estilo.css"
      alignment="CENTER_LEFT">

    <MenuBar HBox.hgrow="ALWAYS" maxWidth="1.7976931348623157E308">
        <Menu mnemonicParsing="false" text="Session">
            <MenuItem fx:id="fxMenuClose" mnemonicParsing="false" text="Close"/>
            <MenuItem fx:id="fxMenuSignOut" mnemonicParsing="false" text="Sign out"/>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
            <MenuItem fx:id="fxMenuAbout" mnemonicParsing="false" text="About"/>
            <MenuItem fx:id="fxMenuContent" mnemonicParsing="false" text="Help"/>
        </Menu>
        <Menu mnemonicParsing="false" text="Accion">
            <MenuItem fx:id="fxCreateCustomer" mnemonicParsing="false" text="Add"/>
            <MenuItem fx:id="fxDeleteCustomer" mnemonicParsing="false" text="Delete"/>
            <MenuItem fx:id="fxUpdateCustomer" mnemonicParsing="false" text="Update"/>
        </Menu>

    </MenuBar>
</HBox>